{
  "name": "@nodepkg/openapi-playground",
  "version": "0.0.0",
  "monobundle": {
    "build": {
      "clean": true
    },
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "dependencies": {
    "@innoai-tech/fetcher": "^0.4.0",
    "@innoai-tech/lodash": "^0.2.0",
    "@innoai-tech/reactutil": "^0.5.3",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.13",
    "@nodepkg/codemirror": "workspace:*",
    "@nodepkg/jsonschema": "workspace:*",
    "@nodepkg/markdown": "workspace:*",
    "@nodepkg/runtime": "workspace:*"
  },
  "peerDependencies": {
    "@types/react": "18.x",
    "core-js": "*",
    "react": "18.x",
    "rxjs": "^7.x"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "monobundle",
    "test": "vitest --run --passWithNoTests",
    "prepublishOnly": "pnpm run build",
    "lint": "prettier --write ."
  },
  "type": "module"
}