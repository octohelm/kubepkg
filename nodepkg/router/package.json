{
  "name": "@nodepkg/router",
  "version": "0.0.0",
  "monobundle": {
    "build": {
      "clean": true
    },
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "dependencies": {
    "@nodepkg/runtime": "workspace:*",
    "react-router": "^6.9.0",
    "react-router-dom": "^6.9.0"
  },
  "peerDependencies": {
    "@types/react": "18.x",
    "react": "18.x",
    "rxjs": "7.x"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "monobundle",
    "test": "vitest --run --passWithNoTests",
    "prepublishOnly": "pnpm run build",
    "lint": "prettier --write ."
  },
  "type": "module"
}