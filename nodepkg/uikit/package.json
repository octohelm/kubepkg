{
  "name": "@nodepkg/uikit",
  "version": "0.0.0",
  "monobundle": {
    "build": {
      "clean": true
    },
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "dependencies": {
    "@emotion/styled": "11.x",
    "@emotion/react": "11.x"
  },
  "peerDependencies": {
    "react": "18.x"
  },
  "devDependencies": {
    "@types/react": "~18.0.28",
    "@types/react-dom": "~18.0.11"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "monobundle",
    "test": "vitest --run --passWithNoTests",
    "prepublishOnly": "pnpm run build",
    "lint": "prettier --write ."
  },
  "type": "module"
}