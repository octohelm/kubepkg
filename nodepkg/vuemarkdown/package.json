{
  "name": "@nodepkg/vuemarkdown",
  "version": "0.1.0",
  "monobundle": {
    "build": {
      "clean": true
    },
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "dependencies": {
    "@nodepkg/runtime": "workspace:^",
    "hast-util-to-jsx-runtime": "^2.0.0",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "unified": "^11.0.1",
    "@innoai-tech/vuekit": "^0.6.3"
  },
  "peerDependencies": {},
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "license": "MIT",
  "scripts": {
    "build": "monobundle",
    "test": "vitest --run --passWithNoTests --dir .",
    "prepublishOnly": "pnpm run build",
    "lint": "prettier --write ."
  },
  "type": "module"
}