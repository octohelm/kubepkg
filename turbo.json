{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "lint": {
      "inputs": [
        "src/*",
        "package.json"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "src/*",
        "package.json"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "src/*",
        "package.json"
      ]
    },
    "dev": {
      "inputs": [
        "src/*",
        "package.json"
      ],
      "cache": false
    }
  }
}